{
  "ServerId": 1, // ID сервера, только число, ОДИНАКОВЫХ ID БЫТЬ НЕ ДОЛЖНО
  "ServerIp": "0.0.0.0:27015", // Указываете IP:PORT сервера
  "ServerName": "Server name", // Название сервера отображаемое в базе данных
  "RconPassword": "12345", // Rcon пароль (ОБЯЗАТЕЛЬНО)
  // База данных
  "Host": "host",
  "Database": "Database",
  "User": "User",
  "Password": "Password",
  "Port": "3306",
  // ===========
  "BlockedIdentifiers": { // Блокирует идентификаторы для некоторых команд в которых они доступны
    "css_slay": [
      "@all"
    ]
  },
  "CommandReplacement": { // Позволяет заменить название команды
    //  "css_respawn" : "css_arespawn" //например
  },
  "MaxWarns": 3, // Максимальное число варнов при котором отключается админка
  "WebApiKey": "", // Указываете свой WebApiKey - нужен для того что бы брать ники из стима при оффлайн бане
  "AdvancedKick": true, // Показывать ли табличку перед киком игрока с сервера (Кик, Бан)
  "AdvancedKickTime": 5, // Время через которое игрока кикнет
  "DebugMode": false, // Дебаг мод, не включайте если не знаете для чего (МНОГО СПАМА В КОНСОЛЬ)
  "MenuType": 2, // -1 = Default(Player select) [MM] | 0 = ChatMenu | 1 = ConsoleMenu | 2 = HtmlMenu | 3 = ButtonMenu [MM] (MM - MenuManager)
  "PermissionReplacement": { // Замена дефолтных флагов прав
    // "admins_manage.list": "l" // в таком случае команду css_am_list можно использовать с флагом l
  },
  "IgnoreCommandsRegistering": [ // Эти команды не будут регестрироватся через API админки (писать без css_ префикса)
    // "admin" // если так сделать команды css_admin просто не будет
  ],
  "MirrorsIp": [ // Айпи зеркал, будут игнорироватся плагином
    "0.0.0.0"
  ],
  "LastPunishmentTime": 172800, // Последние наказания за это время буду отображаться в снятии бана и оффлайн наказаний в ообщем (в секундах)
  "UseOnlineAdminsName": false, // Использовать ли текущий ник админа если он онлайн (Для Announce)
  "AutoUpdateDatabaseNames": false // Обновлять ли ники админов в базе данных на текущие при подключении
}